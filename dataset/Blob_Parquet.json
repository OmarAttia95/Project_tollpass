			"referenceName": "Delta_blobs",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"event_date": {
				"type": "string",
				"defaultValue": "@formatDateTime(utcNow(), 'yyyy-MM-dd')"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
