{
	"name": "Parquet_TollEvents",
	"properties": {
		"linkedServiceName": {
			"referenceName": "Delta_blobs",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"event_date": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": ".*parquet",
					"type": "Expression"
				},
				"folderPath": {
					"value": "toll-events-processed/event_date=@{dataset().event_date}",
					"type": "Expression"
				},
				"fileSystem": "delta"
			},
			"compressionCodec": "none"
		},
		"schema": [
			{
				"name": "transaction_id",
				"type": "INT64"
			},
			{
				"name": "vehicle_id",
				"type": "UTF8"
			},
			{
				"name": "license_plate",
				"type": "UTF8"
			},
			{
				"name": "vehicle_type",
				"type": "UTF8"
			},
			{
				"name": "toll_booth_id",
				"type": "UTF8"
			},
			{
				"name": "entry_point",
				"type": "UTF8"
			},
			{
				"name": "exit_point",
				"type": "UTF8"
			},
			{
				"name": "timestamp",
				"type": "UTF8"
			},
			{
				"name": "payment_method",
				"type": "UTF8"
			},
			{
				"name": "toll_amount",
				"type": "DOUBLE"
			},
			{
				"name": "tag_valid",
				"type": "BOOLEAN"
			},
			{
				"name": "fraud_detected",
				"type": "BOOLEAN"
			}
		]
	}
}